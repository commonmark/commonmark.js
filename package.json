  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "test": "node ./test/test"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "entities": "~1.1.1",
    "mdurl": "~1.0.1",
    "minimist": "~1.2.0",
    "rollup": "^1.29.0",
    "string.prototype.repeat": "^0.2.0"
  },
  "directories": {
    "lib": "./lib"
  },
  "engines": {
    "node": "*"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.1",
    "benchmark": "^2.1.4",
    "bower": "^1.8.8",
    "cached-path-relative": "^1.0.2",
    "eslint": "^4.19.1",
    "http-server": "^0.11.1",
    "lodash": "^4.17.13",
    "markdown-it": "^10.0",
    "marked": "^0.8.0",
    "mem": ">=4.0.0",
    "showdown": "^1.9.1",
    "uglify-js": "^3.4.0"
  }
}
